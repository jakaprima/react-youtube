{
  "name": "nodejs-ex",
  "version": "0.0.1",
  "description": "Node.js sample app for OpenShift 3",
  "main": "dist/server.js",
  "postcss": {},
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-pug": "^4.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.30.1",
    "postcss-loader": "^2.1.0",
    "react-hot-loader": "^3.1.3",
    "sass-loader": "^6.0.6",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.5",
    "webpack-dev-server": "^2.11.1",
    "webpack-hot-middleware": "^2.21.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.3",
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "lodash": "^4.17.5",
    "material-ui": "^0.20.0",
    "mocha": "^2.4.5",
    "mongodb": "^2.1.16",
    "morgan": "^1.7.0",
    "object-assign": "4.1.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-promise": "^0.5.3",
    "winston": "^2.4.0",
    "youtube-api-search": "0.0.5"
  },
  "engine": {
    "node": "*",
    "npm": "*"
  },
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development node src/tmp/server.js",
    "start": "NODE_ENV=production node dist/server.js",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "prebuild": "rimraf dist && rimraf src/tmp",
    "build:client": "cross-env NODE_ENV=production webpack -p --display-error-details --config=config/webpack.config.prod.js",
    "build:server": "copyfiles -a -u 1 src/server/**/**/* lib && babel lib/server -d dist --presets es2015,stage-2 && copyfiles -a -u 1 dist/server.js src/tmp && copyfiles -a -u 1 dist/middlewares/**/**/* src/tmp"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/openshift/nodejs-ex.git"
  },
  "author": "Steve Speicher <sspeiche@gmail.com>",
  "license": "CC-BY-1.0",
  "bugs": {
    "url": "http://github.com/openshift/nodejs-ex/issues"
  },
  "homepage": "http://github.com/openshift/nodejs-ex"
}
